var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');
var prompt = function(texto){return lines.shift();};

let N = parseInt(prompt("NÃºmero de linhas"));
let cont = 1;
let mult = 0;
let aux = 0;
let mult2 = 0;

for(i = 0; i<N; i++){
    mult = cont * cont;
    aux = mult;
    mult2 = cont * aux;
    console.log(cont + " " + aux + " " + mult2);
    cont++;
}
